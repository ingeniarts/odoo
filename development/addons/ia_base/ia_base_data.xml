<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="0">

        <record id="lang_fr_CA" model="res.lang">
            <field name="name">French (CA) / Français (CA)</field>
            <field name="code">fr_CA</field>
            <field name="iso_code">fr_CA</field>
            <field name="translatable">True</field>
            <field name="grouping">[3,0]</field>
            <field name="decimal_point">,</field>
            <field name="thousands_sep"> </field>
            <field name="date_format">%Y-%m-%d</field>
        </record>

        <function name="install_lang" model="res.lang"/>

        <record model="res.partner" context="{'default_is_company': True}">
            <field name="overriden_id" ref="base.main_partner" />
            <field name="name">Ingeniarts Technologies inc.</field>
            <field name="company_id" eval="None"/>
            <field name="image" type="base64" file="ia_base/static/img/ia_logo.png"/>
            <field name="customer" eval="False"/>
            <field name="is_company" eval="True"/>
            <field name="street">10-2435, avenue Watt</field>
            <field name="city">Québec</field>
            <field name="state_id" ref="l10n_ca_toponyms.QC"/>
            <field name="country_id" ref="base.ca"/>
            <field name="zip">G1P 3X2</field>
            <field name="phone">418-905-4004</field>
            <field name="email">info@ingeniarts.com</field>
            <field name="website">http://www.ingeniarts.com/</field>
        </record>

        <record model="res.company">
            <field name="overriden_id" ref="base.main_company" />
            <field name="name">Ingeniarts Technologies inc.</field>
            <field name="partner_id" ref="base.main_partner"/>
            <field name="rml_header1"></field>
            <field name="currency_id" ref="base.CAD"/>
        </record>

    </data>
</openerp>